{
  "name": "auth-service",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "src/public",
      "src/docs"
    ],
    "exec": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./src"
  },
  "scripts": {
    "start": "nodemon",
    "prettier:check": "prettier --debug-check 'src/**/*.ts'",
    "prettier:lint": "prettier --write 'src/**/*.ts'",
    "eslint:check": "eslint . --ext .ts",
    "eslint:lint": "eslint . --ext .ts --quiet --fix",
    "tsc:lint": "tsc --pretty --noEmit",
    "lint:check": "npm-run-all prettier:check eslint:check",
    "lint": "npm-run-all prettier:lint eslint:lint tsc:lint"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "jet-logger": "^1.1.5",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.1.0",
    "morgan": "^1.10.0",
    "objection": "^3.0.1",
    "pg": "^8.7.3"
  }
}
